    <class>com.hallocasa.dataentities.app.UseCase</class>
    <class>com.hallocasa.dataentities.app.User</class>
    <class>com.hallocasa.dataentities.app.test.UserTest</class>
    <class>com.hallocasa.dataentities.app.UserType</class>
    <class>com.hallocasa.dataentities.app.State</class>
    <class>com.hallocasa.dataentities.app.Country</class>
    <class>com.hallocasa.dataentities.app.Token</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="eclipselink.target-database" value="MYSQL"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/hallocasaapp"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.password" value="masterkey"/>
      <property name="eclipselink.ddl-generation" value="none"/>
      <property name="eclipselink.logging.level" value="INFO"/>
    </properties>
  </persistence-unit>
</persistence>
